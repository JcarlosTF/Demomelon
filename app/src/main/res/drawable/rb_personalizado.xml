<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.5dp" android:color="#FFFFFF"></stroke>
            <solid android:color="#EB8532"></solid>
        </shape>
    </item>

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.5dp" android:color="#FFFFFFFF"></stroke>
            <solid android:color="#EB8532"></solid>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.5dp" android:color="#00FFFFFF"></stroke>
            <solid android:color="#FFFFFF"></solid>
        </shape>
    </item>


</selector>